Prefix(:=<http://www.example.com/sigonOntology#>)
Prefix(owl:=<http://www.w3.org/2002/07/owl#>)
Prefix(rdf:=<http://www.w3.org/1999/02/22-rdf-syntax-ns#>)
Prefix(xml:=<http://www.w3.org/XML/1998/namespace>)
Prefix(xsd:=<http://www.w3.org/2001/XMLSchema#>)
Prefix(rdfs:=<http://www.w3.org/2000/01/rdf-schema#>)


Ontology(<http://www.example.com/sigonOntology>

Declaration(ObjectProperty(:hates))
Declaration(ObjectProperty(:loves))
Declaration(NamedIndividual(:bean))
Declaration(NamedIndividual(:jack))
Declaration(NamedIndividual(:jhon))
Declaration(NamedIndividual(:paul))


############################
#   Named Individuals
############################

# Individual: :bean (:bean)

ClassAssertion(owl:Thing :bean)

# Individual: :jack (:jack)

ClassAssertion(owl:Thing :jack)
ObjectPropertyAssertion(:hates :jack :bean)

# Individual: :jhon (:jhon)

ClassAssertion(owl:Thing :jhon)
ObjectPropertyAssertion(:loves :jhon :paul)

# Individual: :paul (:paul)

ClassAssertion(owl:Thing :paul)


)